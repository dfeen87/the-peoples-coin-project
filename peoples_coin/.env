# Database configuration
DB_PATH=instance/peoples_coin.db

# Redis configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Logging level
LOG_LEVEL=INFO

# AILEE engine configuration
AILEE_LOOP_DELAY=5
AILEE_MAX_WORKERS=2

# Immune System configuration
QUARANTINE_TIME_SEC=300
MAX_INVALID_ATTEMPTS=5
RATE_LIMIT_WINDOW_SEC=60
MAX_REQUESTS_PER_WINDOW=30
BLACKLIST_EXPIRY_SEC=3600

# Ports for subsystems
NERVOUS_PORT=5001
SKELETON_PORT=5002
COGNITIVE_PORT=5003

